[
  {
    "api": "ens/testnet/erc20.eth",
    "constants": "./constants.json"
  },
  {
    "query": "./queries/name.graphql",
    "variables": {
      "network": "testnet",
      "address": "$Erc20Addr"
    }
  },
  {
    "query": "./queries/symbol.graphql",
    "variables": {
      "network": "testnet",
      "address": "$Erc20Addr"
    }
  },
  {
    "query": "./queries/totalSupply.graphql",
    "variables": {
      "network": "testnet",
      "address": "$Erc20Addr"
    }
  },
  {
    "query": "./queries/approve.graphql",
    "variables": {
      "network": "testnet",
      "address": "$Erc20Addr",
      "spender": "$address1",
      "amount": "100000000000"
    }
  },
  {
    "query": "./queries/allowance.graphql",
    "variables": {
      "network": "testnet",
      "address": "$Erc20Addr",
      "owner": "$address0",
      "spender": "$address1"
    }
  },
  {
    "query": "./queries/decreaseAllowance.graphql",
    "variables": {
      "network": "testnet",
      "address": "$Erc20Addr",
      "spender": "$address1",
      "subtractedValue": "50000000000"
    }
  }
]
